{
    "version": 4,
    "configurations": [
        {
            "name": "arm-none-eabi",
            "includePath": [
                "${workspaceFolder}/inc/**",
                "${workspaceFolder}/tmp/**",
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/hw/duino/Drivers/CMSIS/Include",
                "${workspaceFolder}/hw/duino/Drivers/CMSIS/Device/ST/STM32F4xx/Include"
            ],
            "forcedInclude": [
                "${workspaceFolder}/hw/duino/Drivers/CMSIS/Include/core_cm4.h"
            ],
            "defines": [
                "USE_FULL_LL_DRIVER",
                "HSE_VALUE=25000000",
                "HSE_STARTUP_TIMEOUT=100",
                "LSE_STARTUP_TIMEOUT=5000",
                "LSE_VALUE=32768",
                "EXTERNAL_CLOCK_VALUE=12288000",
                "HSI_VALUE=16000000",
                "LSI_VALUE=32000",
                "VDD_VALUE=3300",
                "PREFETCH_ENABLE=1",
                "INSTRUCTION_CACHE_ENABLE=1",
                "DATA_CACHE_ENABLE=1",
                "STM32F405xx"
            ],
            "cStandard": "gnu11",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "gcc-x64",
            "compilerPath": "/usr/bin/arm-none-eabi-gcc"
        }
    ]
}
