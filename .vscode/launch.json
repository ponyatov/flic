    "version": "0.2.0",
    "configurations": [
        {
            "name": "STM32F407G-DISC1",
            "type": "cortex-debug",
            "servertype": "openocd",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceFolder}/tmp/STM32F407G-DISC1/${workspaceFolderBasename}.elf",
            "toolchainPrefix": "arm-none-eabi",
            "gdbPath": "/usr/bin/gdb-multiarch",
            "overrideLaunchCommands": [
                "target remote :12345",
                "file ${workspaceFolder}/tmp/STM32F407G-DISC1/${workspaceFolderBasename}.elf",
            //     "add-symbol-file ${workspaceFolder}/tmp/STM32F407G-DISC1/${workspaceFolderBasename}.elf 0x08000000",
            //     "enable breakpoint",
            //     "monitor reset"
            ],
            // "preLaunchTask": "espruino: STM32F4DISCOVERY",
            "svdFile": "${userHome}/stm32/hw/svd/stm32f4/STM32F407.svd",
            // "runToEntryPoint": "Reset_Handler",
            "configFiles": [
                "openocd.cfg"
            ]
            // ,"gdbTarget": "localhost:12345"
            // ,"showDevDebugOutput": "raw"
        }
    ]
}
