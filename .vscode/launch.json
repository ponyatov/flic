    "version": "0.2.0",
    "configurations": [
        {
            "name": "stm32f4discovery",
            "type": "cortex-debug",
            "servertype": "openocd",
            "request": "launch",
            "executable": "${workspaceFolder}/tmp/STM32F407G-DISC1/${workspaceFolderBasename}.elf",
            "gdbPath": "/usr/bin/gdb-multiarch",
            "toolchainPrefix": "arm-none-eabi",
            // "preLaunchTask": "espruino: STM32F4DISCOVERY",
            "svdFile": "${userHome}/stm32/hw/svd/stm32f4/STM32F407.svd",
            "runToEntryPoint": "Reset_Handler",
            "postLaunchCommands": [ "interrupt" ],
            "configFiles": [
                // "board/stm32f4discovery.cfg",
                "interface/stlink-v2.cfg",
                // "interface/stlink.cfg",
                "target/stm32f4x.cfg",
                "hw/STM32F4DISCOVERY.cfg"
            ]
            // ,"showDevDebugOutput": "raw"
        }
    ]
}
